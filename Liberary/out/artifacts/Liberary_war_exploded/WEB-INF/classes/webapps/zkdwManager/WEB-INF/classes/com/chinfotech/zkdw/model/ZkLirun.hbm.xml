<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.chinfotech.zkdw.model">
	<class
		name="Lirun"
		table="zk_lirun"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="Id"
			type="integer"
			column="id"
		>
			<generator class="native"/>
		</id>

		<property
			name="AreaCode"
			column="area_code"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="Lirun"
			column="lirun"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="Date"
			column="date"
			type="string"
			not-null="false"
			length="100"
		/>


	</class>	
</hibernate-mapping>