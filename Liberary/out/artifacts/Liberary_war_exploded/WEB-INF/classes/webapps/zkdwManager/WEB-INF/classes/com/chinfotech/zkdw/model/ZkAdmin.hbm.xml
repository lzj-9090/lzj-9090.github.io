<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.chinfotech.zkdw.model">
	<class
		name="Admin"
		table="zk_admin"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="Id"
			type="integer"
			column="admin_id"
		>
			<generator class="native"/>
		</id>

		<property
			name="RoleId"
			column="role_id"
			type="integer"
			not-null="true"
			length="8"
		/>
		<property
			name="AdminName"
			column="admin_name"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="AdminCode"
			column="admin_code"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="Password"
			column="password"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="Gender"
			column="gender"
			type="java.lang.Byte"
			not-null="true"
			length="1"
		/>
		<property
			name="NickName"
			column="nick_name"
			type="string"
			not-null="false"
			length="30"
		/>
		<property
			name="Email"
			column="email"
			type="string"
			not-null="false"
			length="30"
		/>
		<property
			name="HeadImg"
			column="head_img"
			type="string"
			not-null="false"
			length="255"
		/>
		<property
			name="RegisterTime"
			column="register_time"
			type="timestamp"
			not-null="true"
			length="19"
		/>
		<property
			name="AccessCounts"
			column="access_counts"
			type="integer"
			not-null="true"
			length="8"
		/>
		<property
			name="LastAccessTime"
			column="last_access_time"
			type="timestamp"
			not-null="false"
			length="19"
		/>
		<property
			name="LastAccessIp"
			column="last_access_ip"
			type="string"
			not-null="false"
			length="40"
		/>
		<property
			name="FailCounts"
			column="fail_counts"
			type="integer"
			not-null="false"
			length="8"
		/>
		<property
			name="LastFailIp"
			column="last_fail_ip"
			type="string"
			not-null="false"
			length="40"
		/>
		<property
			name="LastFailTime"
			column="last_fail_time"
			type="timestamp"
			not-null="false"
			length="19"
		/>
		<property
			name="IsValid"
			column="is_valid"
			type="java.lang.Byte"
			not-null="true"
			length="1"
		/>
		<property
			name="IsDisabled"
			column="is_disabled"
			type="java.lang.Byte"
			not-null="true"
			length="1"
		/>
		<property
			name="Description"
			column="description"
			type="string"
			not-null="false"
			length="255"
		/>
		<property
			name="Memo"
			column="memo"
			type="string"
			not-null="false"
			length="255"
		/>


	</class>	
</hibernate-mapping>